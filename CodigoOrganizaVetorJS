<body>

  <script type="text/javascript">
    var vetor = [4,3,5,7,6,8,1,2];

    function ordena(){
     
    var i; 
    var j;
    var aux;
     
              for( i=0; i<8; i++ ){
                      for( j=i+1; j<8; j++ ){
                           if( vetor[i] > vetor[j] ){
                               aux = vetor[i]; 
                               vetor[i] = vetor[j];
                               vetor[j] = aux;
                           }
                      }
               }
       console.log(vetor);        
    }

    ordena();
  </script>

